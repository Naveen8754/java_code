  
   import java.util.*;
public class Main {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
         int i,j,count=0;
        for( i=a;i<=b;i++)
        {
          for( j=2;j<i;j++)
          {
              if(i%j==0)
              break;
          }
              if(i==j)
              count++;
          
            
        }
        System.out.println(count);
	}
	}

	
